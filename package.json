{
    "private": true,
    "name": "matchingclient",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "build": "preact build --no-prerender && cp ./src/public/* ./build/",
        "serve": "./node_modules/.bin/sirv build --port 5234 --cors --single --host",
        "dev": "preact watch",
        "lint": "eslint 'src/**/*.{ts,tsx}'"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "extends": [
            "preact",
            "plugin:@typescript-eslint/recommended"
        ],
        "rules": {
            "@typescript-eslint/explicit-function-return-type": "off",
            "@typescript-eslint/explicit-module-boundary-types": "off",
            "quote-props": "off"
        },
        "ignorePatterns": [
            "build/"
        ]
    },
    "dependencies": {
        "@types/react": "^17.0.13",
        "@types/react-beautiful-dnd": "^13.1.2",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "axios": "^0.24.0",
        "history": "^5.1.0",
        "preact": "^10.3.1",
        "preact-render-to-string": "^5.1.4",
        "preact-transitioning": "^1.3.1",
        "react-beautiful-dnd": "^13.1.0",
        "sirv": "^1.0.17",
        "typescript": "^4.3.5"
    },
    "alias": {
        "react": "preact/compat",
        "react-dom/test-utils": "preact/test-utils",
        "react-dom": "preact/compat",
        "react/jsx-runtime": "preact/jsx-runtime"
    },
    "devDependencies": {
        "eslint": "^7.5.0",
        "eslint-config-preact": "^1.1.1",
        "preact-cli": "^3.0.0",
        "sirv-cli": "^1.0.0-next.3"
    },
    "description": "A Dating/Bullying app, client.",
    "author": "Shady Goat <shady@shadygoat.eu> (https://shadygoat.eu/)"
}
